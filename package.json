{
  "name": "moniqueta-discordx",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon  --ignore src/resources/cache/ --exec ts-node-esm src/main.ts",
    "prisma": "npx prisma migrate dev --name init",
    "studio": "npx prisma studio --port 23532",
    "update": "npx npm-check-updates -u",
    "prettier": "npx  prettier --write .",
    "lint": "npx eslint . --ext .ts,.js --fix"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.3.0",
    "@napi-rs/canvas": "^0.1.30",
    "@prisma/client": "^4.4.0",
    "bufferutil": "^4.0.6",
    "discord.js": "^14.5.0",
    "discordx": "^11.4.0",
    "reflect-metadata": "^0.1.13",
    "utf-8-validate": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
